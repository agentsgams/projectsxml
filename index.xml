            });
        }

        function page(n) {
            document.getElementById('loadpro').style.display = "block"
            const projects = document.getElementById("projects");
            while (projects.firstChild) {
                projects.removeChild(projects.lastChild);
            }

            switch (n) {
                case "p":
                    if (document.getElementById('page').innerText == document.getElementById('pagemax').innerText) {return};
                    document.getElementById('page').innerText = Number(document.getElementById('page').innerText)+1;
                    break;
                case "n":
                    if (document.getElementById('page').innerText == "1") {return};
                    document.getElementById('page').innerText = Number(document.getElementById('page').innerText)-1;
